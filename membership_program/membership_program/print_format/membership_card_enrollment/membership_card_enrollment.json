 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead, footer) -%}\n{% if letter_head and not no_letterhead %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{% endif %}\n<div id=\"footer-html\" class=\"visible-pdf\">\n    {% if not no_letterhead and footer %}\n    <div class=\"letter-head-footer\">\n        {{ footer }}\n    </div>\n    {% endif %}\n</div>\n{% if max_pages > 1 %}\n<p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n{% endif %} {%- endmacro -%} {{ add_header(0,1,doc,letter_head, no_letterhead) }}\n<div>\n    <div align=\"right\">\n        <div><span align=\"left\" style=\"font-size: 22px;\">{{ doc.name }}</span></div>\n    </div>\n    <hr>\n</div>\n<!-- <div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>CARD DETAILS</span>\n    <br>\n</div> -->\n<div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>CARD DETAILS</span>\n    <br>\n</div>\n<br>\n<table style=\"width:100%\" class=\"\">\n    <tr style=\"width:100%\">\n        <td align=\"left\" style=\"width:50%\">\n            <div id=\"container\" align=\"left\" style=\"width:100%\">\n                <table id=\"erpnext-oralcare-print-info-table\" style=\"width:90%\" class=\"erpnext-oralcare-print-info-table\">\n                    <tr>\n                        <td>CATEGORY:</td>\n\t\t\t<td>\n\t\t\t{% for category in doc.membership_program_categories %}\t\t\t\t\n\t\t\t\t{{ loop.index }}) {{ category.category_name }} \n\t\t\t{% endfor %}\n                        </td>\n                    </tr>\n                    <tr>\n                        <td>MEMBERS:</td>\n                        <td>{{ doc.maximum_members }}</td>\n                    </tr>\n                </table>\n            </div>\n        </td>\n        <td align=\"left\" style=\"width:50%\">\n            <div id=\"container\" align=\"left\" style=\"width:100%\">\n                <table id=\"erpnext-oralcare-print-info-table\" style=\"width:90%\" class=\"erpnext-oralcare-print-info-table\">\n                    <tr>\n                        <td>ISSUED ON:</td>\n                        <td></td>\n                    </tr>\n                    <tr>\n                        <td>VALID TILL:</td>\n                        <td></td>\n                    </tr>\n                </table>\n            </div>\n        </td>\n    </tr>\n</table>\n<br>\n<div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>MEMBERS DETAILS</span>\n    <br>\n</div>\n<table style=\"width:100%\" class=\"table table-bordered table-condensed\">\n    <tr style=\"width:100%\">\n        <th style=\"width:30%\">Name</th>\n        <th style=\"width:10%\">Gender</th>\n        <th style=\"width:15%\">DOB</th>\n        <th style=\"width:25%\">Mobile</th>\n        <th style=\"width:20%\">Relation</th>\n\n    </tr>\n    {% set nummber_members = doc.maximum_members %}\n    {% for i in range(nummber_members) %}\n        <tr>\n            <td style=\"height:35px\"></td>\n            <td style=\"height:35px\"></td>\n            <td style=\"height:35px\"></td>\n            <td style=\"height:35px\"></td>\n            <td style=\"height:35px\"></td>\n\n        </tr>\n    {% endfor %}\n</table>\n<div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>TERMS AND CONDITIONS OF {{ doc.name }} CARD</span>\n    <br>\n</div>\n<br>\n{% if doc.terms%}\n    {{ doc.terms }}\n{% endif %}\n<br>\n<br>\n<br>\n<br>\n<div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>MEMBER'S SIGNATURE</span>\n    <br>\n</div>\n<br>\n<br>\n<br>\n<br>\n<div class=\"erpnext-oralcare-print-inner-headings\">\n    <span>NAME OF HEAD OF FAMILY</span>\n    <br>\n</div>",
